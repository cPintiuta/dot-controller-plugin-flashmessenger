{
    "name": "dotkernel/dot-controller-plugin-flashmessenger",
    "type": "library",
    "description": "DotKernel flash messenger controller plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "DotKernel Team",
            "email": "team@dotkernel.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "laminas/laminas-servicemanager": "^3.3",
        "dotkernel/dot-controller": "^2.0",
        "dotkernel/dot-flashmessenger": "^2.0",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Dot\\Controller\\Plugin\\FlashMessenger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotTest\\Controller\\Plugin\\FlashMessenger\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "dev-develop": "2.1-dev"
        }
    }
}
